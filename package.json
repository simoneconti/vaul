{
  "name": "@simoneconti/vaul",
  "version": "1.0.0",
  "description": "Drawer component for React. (fork of emilkowalski/vaul)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "style.css"
  ],
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "npm run type-check && bunchee && npm run copy-assets",
    "copy-assets": "copyfiles -f ./src/style.css .",
    "dev": "bunchee --watch",
    "dev:test": "npx turbo run dev --filter=test...",
    "format": "prettier --write .",
    "test": "npx playwright test"
  },
  "keywords": [
    "react",
    "drawer",
    "dialog",
    "modal"
  ],
  "author": "Emil Kowalski <e@emilkowal.ski> (original author)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/simoneconti/vaul.git"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@radix-ui/react-dialog": "^1.0.4",
    "@types/node": "20.5.7",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.18",
    "@types/scheduler": "^0.26.0",
    "bunchee": "^5.1.5",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "turbo": "1.6",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0.0 || ^19.0.0-rc",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0.0 || ^19.0.0-rc"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.1"
  }
}
